{
	"name": "蓝",
	"version": "1.0",
	"manifest_version": 2,
	"description": "蓝湖插件",
	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": ["clipboardWrite", "contextMenus", "tabs", "<all_urls>", "storage"],
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.staticfile.org; object-src 'self'"
}
